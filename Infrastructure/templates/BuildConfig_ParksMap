kind: "BuildConfig"
apiVersion: "v1"
metadata:
  name: "parksmap-pipeline"
spec:
  source:
    git:
      uri: "https://github.com/laugimethods/advdev_homework_template"
  strategy:
    jenkinsPipelineStrategy:
      jenkinsfilePath: ParksMap/Jenkinsfile
      env:
        - name: GUID
          value: "${GUID}"
        - name: CLUSTER
          value: "${CLUSTER}"