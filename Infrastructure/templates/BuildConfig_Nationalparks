kind: "BuildConfig"
apiVersion: "v1"
metadata:
  name: "nationalparks-pipeline"
spec:
  source:
    git:
      uri: "https://github.com/laugimethods/advdev_homework_template"
  strategy:
    jenkinsPipelineStrategy:
      jenkinsfilePath: Nationalparks/Jenkinsfile
      env:
        - name: GUID
          value: "${GUID}"
        - name: CLUSTER
          value: "${CLUSTER}"
        - name: FAST_MODE
          value: "${FAST_MODE}"